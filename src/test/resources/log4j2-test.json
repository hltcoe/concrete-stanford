{
    "configuration": {
        "appenders": {
            "Console": {
                "name": "STDOUT",
                "PatternLayout": {
                    "pattern": "%d %-5p %c{1.} [%t] %m%n"
                }
            }
        },
        "loggers": {
            "root": {
                "level": "debug",
                "appender-ref": {
                    "ref": "STDOUT"
                }
            },

            "logger" : [
              {
                "name" : "com.hp",
                "level": "warn",
                  "appender-ref": {
                      "ref": "STDOUT"
                  }
              },
              {
                "name" : "edu.jhu.hlt.concrete.ingesters.bolt",
                "level": "info",
                  "appender-ref": {
                      "ref": "STDOUT"
                  }
              },
              {
                "name" : "org.apache.jena",
                "level": "warn",
                  "appender-ref": {
                      "ref": "STDOUT"
                  }
              }
            ]
        }
    }
}
